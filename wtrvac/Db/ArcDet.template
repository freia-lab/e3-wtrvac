record (bo, "$(SYS)-$(SUBS):$(DEV):$(SIG)") {
  field (DESC, "$(SUBS):$(DEV) $(DESC)")
  field (DTYP, "S7plc")
  field (OUT,  "@PLC:WTRVAC/$(OFFS) T=BYTE B=$(BIT)")
  field (ZNAM, "$(ZNAM)")
  field (HIGH, "1.0")
  field (UDFS, "NO_ALARM")
  field (DOL,  "0")
  field (ONAM, "$(ONAM)")
  field (PINI, "YES")
}

