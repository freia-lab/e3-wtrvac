# Input/output records

record(mbbi, "WtrC-Dwtr:P1:sOpMode") {
  field(DTYP, "S7plc")
  field(INP, "@PLC:WTRVAC/157 T=BYTE")
  field(NOBT, "8")
  field(ZRVL, "0")
  field(ZRST, "AutoControl")
  field(ONVL, "4")
  field(ONST, "OpenLoopMin")
  field(TWVL, "6")
  field(TWST, "OpenLoopMax")
  field(PINI, "YES")
  field(FLNK, "WtrC-Dwtr:P1:cOpMode-rbk")
  field(SCAN, "I/O Intr")
}

